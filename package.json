{
  "name": "health-pool",
  "type": "module",
  "version": "1.0.0",
  "description": "Helps coordinate carpools for people who've just had surgery and therefore cannot drive",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "start": "node server/server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint-fix": "eslint . --ext .html,.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abhinavtripathy/cs326-final-delta.git"
  },
  "author": "CS326 Fall 2020 Group Delta",
  "contributors": [
    {
      "name": "Abhinav Tripathy"
    },
    {
      "name": "Aditya Narayanan"
    },
    {
      "name": "Joseph Black"
    }
  ],
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/abhinavtripathy/cs326-final-delta/issues"
  },
  "homepage": "https://github.com/abhinavtripathy/cs326-final-delta#readme",
  "eslintConfig": {
        "env": {
            "browser": true,
            "es2021": true,
            "node": true
        },
        "extends": "eslint:recommended",
        "plugins": ["html"],
        "settings": {
          "html/report-bad-indent": "error"
        },
        "parserOptions": {
            "ecmaVersion": 12,
            "sourceType": "module"
        },
        "rules": {
            "indent": [
                "error",
                4
            ],
            "quotes": [
                "error",
                "single"
            ],
            "semi": [
                "error",
                "always"
            ],
            "no-var": "error",
            "eqeqeq": "error",
            "curly": [
                "warn",
                "all"
            ],
            "prefer-const": [
                "warn",
                {}
            ],
            "consistent-return": "error"
        }
    },
  "dependencies": {
    "express": "^4.17.1",
    "faker": "^5.1.0"
  },
  "devDependencies": {
    "eslint": "^7.13.0",
    "eslint-plugin-html": "^6.1.0"
  }
}
